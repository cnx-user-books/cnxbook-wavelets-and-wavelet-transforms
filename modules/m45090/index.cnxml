                      </m:mrow>
                    </m:mtd>
                    <m:mtd>
                      <m:mrow>
                        <m:msub>
                          <m:mover accent="true">
                            <m:mi>f</m:mi>
                            <m:mo>˜</m:mo>
                          </m:mover>
                          <m:mn>3</m:mn>
                        </m:msub>
                        <m:mrow>
                          <m:mo>(</m:mo>
