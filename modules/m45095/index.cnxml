                        <m:mo>-</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>)</m:mo>
                        <m:mi>f</m:mi>
                        <m:mo>(</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>)</m:mo>
                        <m:mo>-</m:mo>
                        <m:mi>r</m:mi>
                        <m:mo>(</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>)</m:mo>
                        <m:mi>f</m:mi>
                        <m:mo>(</m:mo>
                        <m:mo>-</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>)</m:mo>
                        <m:mo>,</m:mo>
                      </m:mrow>
                    </m:mtd>
                    <m:mtd columnalign="left">
                      <m:mrow>
                        <m:mtext>if</m:mtext>
                        <m:mspace width="4.pt"/>
                        <m:mrow>
                          <m:mi>t</m:mi>
                          <m:mo>&lt;</m:mo>
                          <m:mn>0</m:mn>
                        </m:mrow>
