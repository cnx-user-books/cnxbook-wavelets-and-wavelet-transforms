(c) <m:math overflow="scroll"><m:mrow><m:mi>f</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo form="prefix">sin</m:mo><m:mo>(</m:mo><m:mfrac><m:mi>π</m:mi><m:mn>4</m:mn></m:mfrac><m:mrow><m:mo>(</m:mo><m:mi>n</m:mi><m:mo>+</m:mo><m:mo>.</m:mo><m:mn>5</m:mn><m:mo>)</m:mo></m:mrow><m:mi>t</m:mi><m:mo>)</m:mo><m:mi>u</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow><m:mi>u</m:mi><m:mrow><m:mo>(</m:mo><m:mn>4</m:mn><m:mo>-</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math> where <m:math overflow="scroll"><m:mrow><m:mi>n</m:mi><m:mo>=</m:mo><m:mn>10</m:mn></m:mrow></m:math><newline/>
(d) <m:math overflow="scroll"><m:mrow><m:msub><m:mi>U</m:mi><m:mo>-</m:mo></m:msub><m:mrow><m:mo>(</m:mo><m:msub><m:mi>r</m:mi><m:mrow><m:mo>[</m:mo><m:mn>3</m:mn><m:mo>]</m:mo></m:mrow></m:msub><m:mo>,</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>)</m:mo></m:mrow><m:msub><m:mi>U</m:mi><m:mo>-</m:mo></m:msub><m:mrow><m:mo>(</m:mo><m:msub><m:mi>r</m:mi><m:mrow><m:mo>[</m:mo><m:mn>3</m:mn><m:mo>]</m:mo></m:mrow></m:msub><m:mo>,</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>)</m:mo></m:mrow><m:mi>f</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math><newline/>
(e) <m:math overflow="scroll"><m:mrow><m:mi>f</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow><m:mo>=</m:mo><m:mo form="prefix">cos</m:mo><m:mo>(</m:mo><m:mfrac><m:mi>π</m:mi><m:mn>4</m:mn></m:mfrac><m:mrow><m:mo>(</m:mo><m:mi>n</m:mi><m:mo>)</m:mo></m:mrow><m:mi>t</m:mi><m:mo>)</m:mo><m:mi>u</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow><m:mi>u</m:mi><m:mrow><m:mo>(</m:mo><m:mn>4</m:mn><m:mo>-</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math> where <m:math overflow="scroll"><m:mrow><m:mi>n</m:mi><m:mo>=</m:mo><m:mn>10</m:mn></m:mrow></m:math><newline/>
(f) <m:math overflow="scroll"><m:mrow><m:msub><m:mi>U</m:mi><m:mo>+</m:mo></m:msub><m:mrow><m:mo>(</m:mo><m:msub><m:mi>r</m:mi><m:mrow><m:mo>[</m:mo><m:mn>3</m:mn><m:mo>]</m:mo></m:mrow></m:msub><m:mo>,</m:mo><m:mn>0</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>)</m:mo></m:mrow><m:msub><m:mi>U</m:mi><m:mo>-</m:mo></m:msub><m:mrow><m:mo>(</m:mo><m:msub><m:mi>r</m:mi><m:mrow><m:mo>[</m:mo><m:mn>3</m:mn><m:mo>]</m:mo></m:mrow></m:msub><m:mo>,</m:mo><m:mn>4</m:mn><m:mo>,</m:mo><m:mn>1</m:mn><m:mo>)</m:mo></m:mrow><m:mi>f</m:mi><m:mrow><m:mo>(</m:mo><m:mi>t</m:mi><m:mo>)</m:mo></m:mrow></m:mrow></m:math><newline/>
