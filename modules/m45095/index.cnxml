              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>r</m:mi>
                <m:mo>,</m:mo>
                <m:mn>0</m:mn>
                <m:mo>,</m:mo>
                <m:mn>1</m:mn>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mi>f</m:mi>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>t</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mfenced separators="" open="{" close="">
                <m:mtable>
                  <m:mtr>
                    <m:mtd columnalign="left">
                      <m:mrow>
                        <m:mo>-</m:mo>
                        <m:mo>-</m:mo>
                        <m:mi>r</m:mi>
                        <m:mo>(</m:mo>
                        <m:mo>-</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>)</m:mo>
                        <m:mi>f</m:mi>
                        <m:mo>(</m:mo>
                        <m:mo>-</m:mo>
                        <m:mi>t</m:mi>
                        <m:mo>)</m:mo>
                        <m:mo>,</m:mo>
                      </m:mrow>
                    </m:mtd>
                    <m:mtd columnalign="left">
                      <m:mrow>
                        <m:mspace width="4.pt"/>
                        <m:mtext>if</m:mtext>
                        <m:mspace width="4.pt"/>
                        <m:mrow>
